{
    "name": "fabpl/laravel-model-status",
    "description": "Laravel package to assign statuses to Eloquent Models",
    "keywords": [
        "laravel",
        "model",
        "status"
    ],
    "license": "MIT",
    "authors": [
      {
        "name": "Fabrice Planchette",
        "email": "planchettefabrice@hotmail.com",
        "homepage": "https://fabpl.github.io",
        "role": "Developer"
      }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.26"
    },
    "require-dev": {
        "orchestra/testbench": "^6.11"
    },
    "autoload": {
        "psr-4": {
            "Fabpl\\ModelStatus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fabpl\\ModelStatus\\ModelStatusServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
